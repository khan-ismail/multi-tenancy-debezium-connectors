{
  "name": "mysql-source-connector-tenant1",
  "config": {
    "connector.class": "io.debezium.connector.mysql.MySqlConnector",
    "database.hostname": "host.docker.internal",
    "database.port": "3306",
    "database.user": "debezium",
    "database.password": "dbz",
    "database.server.id": "223344",
    "database.server.name": "dbserver1",
    "database.include.list": "ejabberd",
    "include.schema.changes": "false",
    "topic.prefix": "dbserver1",

    "producer.override.topic.creation.default.replication.factor": "1",
    "producer.override.topic.creation.default.partitions": "1",
    "topic.creation.default.replication.factor": "1",
    "topic.creation.default.partitions": "1",

    "producer.override.max.request.size": "8388608",
    "producer.override.buffer.memory": "134217728",
    "producer.override.compression.type": "snappy",
    "producer.override.linger.ms": "100",

    "snapshot.mode": "initial",
    "snapshot.locking.mode": "minimal",
    "snapshot.fetch.size": "1000",
    "max.batch.size": "500",

    "database.history.kafka.bootstrap.servers": "kafka:9092",
    "schema.history.internal.kafka.bootstrap.servers": "kafka:9092",
    "schema.history.internal.kafka.topic": "schema-changes.ejabberd",

    "time.precision.mode": "connect",
    "decimal.handling.mode": "double",
    "binary.handling.mode": "base64",

    "transforms": "AddTenant",
    "transforms.AddTenant.type": "org.apache.kafka.connect.transforms.InsertField$Value",
    "transforms.AddTenant.static.field": "tenant_id",
    "transforms.AddTenant.static.value": "cx-dev.masdr.org"
  }
}
